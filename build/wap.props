<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <SharedWapPropsImported>true</SharedWapPropsImported>

    <!-- Output paths -->
    <LayoutDir Condition="'$(LayoutDir)'==''">$(ProjectOutRoot)appx</LayoutDir>
    <MSBuildProjectExtensionsPath>$(IntDir)</MSBuildProjectExtensionsPath>
    <GenerateProjectSpecificOutputFolder>false</GenerateProjectSpecificOutputFolder>

    <!-- Build -->
    <TargetPlatformVersion>10.0.$(WinSdkRelease10).0</TargetPlatformVersion>
    <TargetPlatformMinVersion>10.0.$(WinSdkRelease10).0</TargetPlatformMinVersion>
    <AppxPackageSigningEnabled>false</AppxPackageSigningEnabled>
    <NoWarn>$(NoWarn);NU1702</NoWarn>

  </PropertyGroup>
</Project>
