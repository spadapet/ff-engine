<Window
    x:Class="designer.main_window"
    x:ClassModifier="internal"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Title="Resource Editor Designer"
    Width="960"
    Height="540"
    Background="{DynamicResource Brush.Background.Popup}"
    Foreground="{DynamicResource Brush.Foreground.Normal}"
    FontFamily="{StaticResource Font.Family.Default}"
    FontSize="{StaticResource Font.Size.Normal}">
    <Grid>
        <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center">Text Testing</TextBlock>
    </Grid>
</Window>
